{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  },
  "AgentURL": "https://httpbin.org/",
  "AllowedHosts": "*",
  "ConnectionStrings": {
      "DatabaseConnection": "Data Source=/database/HubDatabase.db"
  },
  "EnableAutoMigration": true,
  "StatusHandlerFrequency": 30,
  "InactiveAgentThresholds": {
    "Deleted": 30,
    "Disconnected": 365
  }
}
