{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  },
  "TidyEvents": {
    "Address": "hub-tidy-events",
    "Port": 5057
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
      "DatabaseConnection": "Data Source=./Database/HubDatabase.db"
  },
  "EnableAutoMigration": true,
  "StatusHandlerFrequency": 10,
  "InactiveAgentThresholds": {
    "Deleted": 30,
    "Disconnected": 365
  }
}
