{
    "Logging": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Kestrel": {
      "EndpointDefaults": {
        "Protocols": "Http2"
      },
      "EndPoints": {
        "Http": {
          "Url": "http://0.0.0.0:5000"
        }
      }
    },
    "AuthInterceptor": {
      "HubUrl": "http://localhost:7001/gateway"
    },
    "ConnectionStrings": {
      "DatabaseConnection": "Host=localhost;Database=db;Username=user;Password=pass"
    },
    "Notion": {
      "AuthToken": "fill_me_up"
    },
    "GoogleDrive": {
      "ServiceAccountKeysPath": "gcloud_service_account_keys.json"
    }
}
