{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "AgentURL": "https://httpbin.org/",
  "AllowedHosts": "*",
  "ConnectionStrings": {
      "DatabaseConnection": "Data Source=DATABASE_NAME.db"
  },
  "EnableAutoMigration": true,
  "InactiveAgentThresholds": {
    "Deleted": 30,
    "Disconnected": 365
  }
}
